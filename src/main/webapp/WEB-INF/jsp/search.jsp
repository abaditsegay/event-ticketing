<form class="form-inline">
	<div class="form-row centered">
		<div class="form-group mb-2">
			<select id="city" class="form-control">
				<option value="all" selected>All Cities</option>
				<option value="alamata">Alamata</option>
				<option value="adigrat">Adigrat</option>
				<option value="maichew">Maichew</option>
			</select>
		</div>
		<div class="form-group mx-sm-3 mb-2">
		<input id="eventdate" placeholder="All dates" class="form-control">
		</div>
		<div class="form-group mx-sm-3 mb-2">
			<select id="event" class="form-control">
				<option value="all" selected>All events</option>
				<option value="sports">Sports</option>
				<option value="concerts">Concerts</option>
				<option value="theatres">Theater</option>
			</select>
		</div>
		<div class="form-group mx-sm-3 mb-2">
			<input id="textsearch" placeholder="Search by text" class="form-control">
		</div>
		<div class="form-group mx-sm-3 mb-2">
			<button class="form-control">Search</button>
		</div>
	</div>
</form>


